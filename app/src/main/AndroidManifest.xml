<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pe.elcomercio.pagoefectivosdkjavasample">

    <application
        android:name=".PagoEfectivoSDKApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ui.SplashActivity"
            android:theme="@style/SplashAppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.GenerateCipActivity" />
        <activity android:name=".ui.SearchCipActivity" />
        <activity
            android:name=".ui.PaymentMethodActivity"
            android:label="@string/method_payment_name" />
        <activity android:name=".ui.WhereToPayActivity" />
    </application>

</manifest>